namespace RiskMan\Entity;
use RiskMan\Entity\MagicClass;
use Doctrine\ORM\Mapping as ORM;

/**
 * Description of book
 *
 * @author rolf
 * @ORM\Entity
 * @ORM\Table(name="book")
 * @property int $id
 * @property string $name
 * 
 */
class Book extends MagicClass
{

    /**
     * @ORM\Id
     * @ORM\Column(type="integer");
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @ORM\Column(type="string", length=64)
     */
    protected $name;



    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name
     *
     * @param string $name
     *
     * @return Book
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }
}
